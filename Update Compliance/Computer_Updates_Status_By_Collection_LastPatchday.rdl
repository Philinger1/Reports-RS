    <DataSet Name="Update_Classification_DS">
      <Query>
        <DataSourceName>DataSource4</DataSourceName>
        <CommandText>SELECT DISTINCT
	CategoryInstanceName
FROM 
	v_CICategories_All catall2 
	join v_CategoryInfo catinfo2 on catall2.CategoryInstance_UniqueID = catinfo2.CategoryInstance_UniqueID and catinfo2.CategoryTypeName='UpdateClassification'
where

	CategoryInstanceName != 'Upgrades'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CategoryInstanceName">
          <DataField>CategoryInstanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Machine_Name_DS">
      <Query>
        <DataSourceName>DataSource4</DataSourceName>
        <CommandText>select 
	SYS.Netbios_Name0 as 'NetbiosName', 
	SYS.Name0 as 'Name'

from 
	v_R_System SYS
	JOIN v_FullCollectionMembership FCM on SYS.ResourceID = FCM.ResourceID 
	JOIN v_Collection Col on Col.CollectionID = FCM.CollectionID 

Where 
	
	SYS.Client0 = 1
	AND 
	SYS.Active0 = 1

ORDER BY
	SYS.Netbios_Name0,
	SYS.Name0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="NetbiosName">
          <DataField>NetbiosName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
